{
    "name": "@adamburgess/linq",
    "version": "0.0.1",
    "description": "A linq library.",
    "scripts": {
        "tsd:esm": "tsc --build tsconfig-publish-esm.json", 
        "build:esm": "esbuild src/*.ts src/**/*.ts --platform=node --outdir=dist",
        "publish:build": "rm -rf dist dist-cjs && nr build:esm && nr tsd:esm",
        "publish:copy": "rm -rf publish && mkdir -p publish && cp -r dist/* package.json LICENSE README.md publish && node scripts/publish",
        "publish": "nr publish:build && nr publish:copy && cd publish && npm publish --dry-run",
        "publish:__GO__": "nr publish && cd publish && npm publish --access=public",
        "build:dev": "esbuild --log-level=warning src/*.ts src/**/*.ts test/*.ts --platform=node --sourcemap --outdir=dist-dev",
        "test": "c8 --r lcov -- pta dist-dev/test/*.test.js",
        "bt": "nr build:dev && nr test && tsc",
        "watch": "nodemon -w src -w test -e ts -x nr -- bt"
    },
    "type": "module",
    "types": "linq.d.ts",
    "main": "linq.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/adamburgess/linq.git"
    },
    "keywords": [
        "linq",
        "iterator"
    ],
    "author": "Adam Burgess",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/adamburgess/linq/issues"
    },
    "homepage": "https://github.com/adamburgess/linq#readme",
    "devDependencies": {
        "@adamburgess/nr": "^1.0.1",
        "@types/node": "^14.14.35",
        "@types/sinon": "^9.0.11",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "c8": "^7.6.0",
        "delay": "^5.0.0",
        "esbuild": "^0.9.6",
        "eslint": "^7.22.0",
        "nodemon": "^2.0.7",
        "pta": "^0.2.1",
        "sinon": "^9.2.4",
        "source-map-support": "^0.5.19",
        "typescript": "^4.3.0-dev.20210319",
        "zora": "^4.0.2"
    }
}